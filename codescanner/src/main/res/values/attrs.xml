<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScannerOverlay">
        <attr name="square_width" format="integer" />
        <attr name="square_height" format="integer" />
        <attr name="line_color" format="color" />
        <attr name="line_width" format="integer" />
        <attr name="line_speed" format="integer" />
    </declare-styleable>

    <declare-styleable name="BarcodeReaderFragment">
        <attr name="auto_focus" format="boolean" />
        <attr name="use_flash" format="boolean" />
    </declare-styleable>

    <declare-styleable name="rsv">
        <attr name="rsv_show_text" format="boolean" />
        <attr name="rsv_draw" format="boolean" />
        <attr name="rsv_multiple" format="boolean" />
        <attr name="rsv_touch" format="boolean" />
        <attr name="rsv_auto_focus" format="boolean" />
        <attr name="rsv_flash" format="boolean" />
        <attr name="rsv_rect_colors" format="reference" />
        <attr name="rsv_camera_facing" format="enum">
            <enum name="back" value="0"></enum>
            <enum name="front" value="0"></enum>
        </attr>
        <attr name="rsv_code_format">
            <flag name="all_format" value="0"></flag>
            <flag name="code_128" value="1"></flag>
            <flag name="code_39" value="2"></flag>
            <flag name="code_93" value="4"></flag>
            <flag name="code_bar" value="8"></flag>
            <flag name="data_matrix" value="16"></flag>
            <flag name="ean_13" value="32"></flag>
            <flag name="ean_8" value="64"></flag>
            <flag name="itf" value="128"></flag>
            <flag name="qr_code" value="256"></flag>
            <flag name="upc_a" value="512"></flag>
            <flag name="upc_e" value="1024"></flag>
            <flag name="pdf417" value="2028"></flag>
            <flag name="aztec" value="4029"></flag>
        </attr>

    </declare-styleable>
</resources>